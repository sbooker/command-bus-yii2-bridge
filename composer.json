{
    "name": "sbooker/command-bus-yii2-bridge",
    "description": "Bridge for Yii2 Active Record",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Knigin",
            "email": "sergey.knigin.dev@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "sbooker/command-bus": "0.1.*",
        "sbooker/yii2-ar-transaction-handler": ">=0.1.1 <1.0.0",
        "laminas/laminas-hydrator": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Sbooker\\CommandBus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Sbooker\\CommandBus\\Infrastructure\\Persistence\\Yii2\\": "tests/",
            "Tests\\Sbooker\\CommandBus\\": "vendor/sbooker/command-bus/tests"
        }
    }
}
