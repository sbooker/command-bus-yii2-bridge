{
    "name": "sbooker/command-bus-yii2-bridge",
    "description": "Bridge for Yii2 Active Record",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Knigin",
            "email": "sergey.knigin.dev@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-json": "*",
        "sbooker/command-bus": "0.6.*",
        "sbooker/yii2-ar-transaction-handler": "1.0.*",
        "laminas/laminas-hydrator": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Sbooker\\CommandBus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Sbooker\\CommandBus\\Infrastructure\\Persistence\\Yii2\\": "tests/",
            "Tests\\Sbooker\\CommandBus\\": "vendor/sbooker/command-bus/tests"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "sort-packages": true,
    "allow-plugins": {
        "yiisoft/yii2-composer": true
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
